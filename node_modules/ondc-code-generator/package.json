{
	"name": "ondc-code-generator",
	"version": "0.3.7",
	"description": "generate code from build.yaml ",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"build": "tsc && copyfiles -u 1 \"src/**/*.{yaml,html,css,mustache}\" dist/",
		"start": "node ./dist/index.js",
		"dev": "nodemon",
		"clean": "rm -rf dist",
		"prepare": "npm run clean && npm run build"
	},
	"author": "extedcoud",
	"license": "MIT",
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/js-yaml": "^4.0.9",
		"@types/json-schema": "^7.0.15",
		"@types/jsonpath": "^0.2.4",
		"@types/mustache": "^4.2.5",
		"@types/node": "^22.10.1",
		"copyfiles": "^2.4.1",
		"esm-loader-typescript": "^1.0.6",
		"nodemon": "^3.1.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@apidevtools/json-schema-ref-parser": "^11.7.2",
		"chalk": "^4.1.2",
		"chevrotain": "^11.0.3",
		"fs-extra": "^11.2.0",
		"js-yaml": "^4.1.0",
		"json-schema": "^0.4.0",
		"json-to-yaml-ref": "^1.0.0",
		"jsonpath": "^1.1.1",
		"marked": "^15.0.3",
		"mustache": "^4.2.0",
		"prettier": "^3.4.2",
		"winston": "^3.17.0"
	}
}
